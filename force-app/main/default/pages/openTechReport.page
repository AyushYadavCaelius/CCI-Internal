<apex:page standardController="Technology__c" recordSetVar="tech">
    <apex:slds />
    <div class="slds-scope">
    <apex:form id="form" style="height: 200px;">
        <!-- SPINNER -->
                <div id="spinner" class="slds-spinner_container " >
                    <div role="status" class="slds-spinner slds-spinner--large slds-spinner--brand">
                        <div class="slds-spinner__dot-a"></div>
                        <div class="slds-spinner__dot-b"></div>
                    </div>
                </div>
        <!-- / SPINNER -->
    </apex:form>
    </div>

    <script>
        setTimeout(function(){ document.getElementById('spinner').className += ' slds-hide' }, 5000);
    </script>
    
    
    <script>
        window.onload = function() {
            var reportId = '{!$Label.TechReportId}';   
            var reportUrl = '/lightning/r/Report/' + reportId + '/view?queryScope=userFolders';
            window.open(reportUrl, '_blank');
            history.back();
        };
    </script>
</apex:page>